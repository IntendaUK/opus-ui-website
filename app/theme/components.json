			"color": {
				"dft": "primaryText"
			}
		}
	},
	"localStorageManager": {
		"path": "localStorageManager"
	},
	"markdownLabel": {
		"path": "markdownLabel",
		"propSpec": {
			"allowScriptAnchors": {
				"dft": true
			},
			"overrideStyles": {
				"link": {
					
				}
			},
			"style": {
				"dft": {
					".cpnMarkdownLabel": {
						"*": {
							"color": "white",
							"user-select": "text"
						},
						"h1": {
							"font-size": "42px",
							"font-weight": "700",
							"color": "var(--colors-footerText)"
						},
						"h2": {
							"font-weight": "700",
							"color": "var(--colors-footerText)",
							"margin": "1.5rem 0px",
							"line-height": "2.5rem"
						},
						"p": {
							"font-size": "17px",
							"line-height": "30px",
							"color": "var(--colors-footerText)",
							"margin": "1rem 0px",
							"strong": {
								"font-size": "17px"
							}
						},
						"p a": {
							"font-size": "17px",
							"line-height": "30px",
							"color": "var(--colors-primaryOpusBlue)",
							"margin": "1rem 0px"
						},
						"ul, ol": {
							"margin": "18px 0px"
						},
						"ul li, ol li": {
							"font-size": "17px",
							"line-height": "22px",
							"color": "var(--colors-footerText)",
							"margin": "1rem 0px"
						}
					}
				}
			}
		}
	},
	"modal": {
		"path": "modal"
	},
	"notifications": {
		"path": "notifications"
	},
	"popup": {
		"path": "popup",
		"propSpec": {
			"lookupPrps": {
