		"propSpec": {
			"popupBackgroundColor": {
				"dft": "primaryWhite"
			},
			"popupDateColorSelected": {
				"dft": "primaryWhite"
			},
			"popupDateBackgroundColorSelected": {
				"dft": "primaryDeloitteGreen"
			},
			"popupTodayColor": {
