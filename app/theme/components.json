	"input": {
		"path": "input",
		"propSpec": {
			"color": {
				"dft": "white"
			},
			"colorPlaceholder": {
				"dft": "secondaryText"
			},
			"focusLine": {
				"dft": false
			},
			"errorLine": {
				"dft": false
			},
			"focusLineColor": {
				"dft": "primary"
			},
			"colorPlaceholder": {
				"dft": "white"
			}
		}
	},
	"label": {
		"path": "label",
		"propSpec": {
