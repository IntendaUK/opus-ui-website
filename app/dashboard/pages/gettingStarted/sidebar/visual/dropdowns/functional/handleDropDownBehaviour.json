{
    "acceptPrps": {
        "rowData": {
            "type": "object",
            "desc": "used to flow data from staticData in index file"
        }
    },
    "prps": {
        "fireScript": {
            "id": "sSN",
            "actions": [
                {
                    "type": "applyComparison",
                    "operator": "isTruthy",
                    "value": "$rowData.children$",
                    "branch": {
                        "true": [
                            {
                                "id": "sEC",
                                "type": "morphIterateArray",
                                "value": "{{eval.JSON.parse(JSON.stringify({{state.||treeview||.expandedNodes}}) )}}",
                                "chain": [
                                    {
                                        "actionCondition": {
                                            "operator": "isNotEqual",
                                            "value": "{{sEC.variable.record}}",
                                            "compareValue": ""
                                        },
                                        "type": "setState",
                                        "target": "||treeview||",
                                        "key": "tToggleParent",
                                        "value": "{{eval.{{state.||treeview||.staticData}}.find(f => f.id === {{variable.record}})}}"
                                    }
                                ]
                            },
                            {
                                "type": "setState",
                                "target": "||treeview||",
                                "key": "tToggleParent",
                                "value": "$rowData$"
                            }
                        ],
                        "false": [
                            {
                                "actionCondition": {
                                    "operator": "isNotEqual",
                                    "value": "$rowData.dataFile$",
                                    "compareValue": "{{sSN.state.||gettingStarted||.selectedLessonId}}"
                                },
                                "type": "setState",
                                "target": "||gettingStarted||",
                                "key": "selectedLessonId",
                                "value": "$rowData.dataFile$"
                            }
                        ]
                    }
                }
            ]
        }
    }
}