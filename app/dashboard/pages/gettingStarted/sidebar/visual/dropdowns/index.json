{
	"acceptPrps": {},
	"scope": "treeview",
	"type": "treeview",
	"traits": [
		{
			"trait": "./functional/staticData"
		}
	],
	"prps": {
		"dtaAtr": "id",
		"disAtr": "name",
		"childAtr": "children",
		"hideRootNode": true,
		"autoExpand": true,
		"renderExpander": false,
		"mdaLabel": {
			"id": "((rowData.id))label",
			"type": "container",
			"traits": [
				{
					"trait": "./functional/handleBackgroundColorBehaviour",
					"traitPrps": {
						"rowData": "{{rowData}}"
					}
				},
				{
					"trait": "./functional/handleDropDownBehaviour",
					"traitPrps": {
						"rowData": "{{rowData}}"
					}
				}
			],
			"prps": {
				"canClick": true,
				"canHover": true
			},
			"wgts": [
				{
					"id": "((rowData.id))inner",
					"traits": [
						{
							"trait": "./visual/iconAndLabel",
							"traitPrps": {
								"rowData": "{{rowData}}"
							}
						}
					]
				}
			]
		},
        "scps": [
            {
                "actions": [
                    {
                        "type": "setState",
                        "key": "tToggleParent",
                        "value": "{{state.self.staticData.0}}"
                    }
                ]
            }
        ]
	}
}