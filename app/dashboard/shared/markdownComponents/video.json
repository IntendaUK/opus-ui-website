{
	"acceptPrps": {
		"value": {
			"type": "string"
		}
	},
	"type": "url",
	"prps": {
		"value": "%value%",
		"width": "50%",
		"style": {
			".cpnUrl": {
				"height": "unset",
				"aspect-ratio": "16 / 9",
				"& + .cpnUrl": {
					"margin-top": "var(--global-mediumPadding)"
				},
				"@media only screen and (max-width: 700px)": {
                    "width": "100% !important"
                }
			}
		}
	}
}